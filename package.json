{
  "name": "@skapxd/tree",
  "version": "1.0.0",
  "description": "A directory structure tree generator for CLI",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "bin": {
    "tree": "./dist/cli.cjs"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest run -c vitest.unit.config.ts",
    "test:ui": "vitest --ui -c vitest.unit.config.ts",
    "test:coverage": "vitest run --coverage -c vitest.unit.config.ts",
    "typecheck": "tsc --noEmit",
    "start": "node dist/cli.cjs"
  },
  "keywords": [
    "directory",
    "tree",
    "cli",
    "ascii",
    "structure"
  ],
  "author": "skapxd",
  "license": "MIT",
  "dependencies": {
    "commander": "14.0.2"
  },
  "devDependencies": {
    "@types/commander": "2.12.5",
    "@types/node": "25.0.10",
    "@vitest/coverage-v8": "4.0.17",
    "@vitest/ui": "4.0.17",
    "tsup": "8.5.1",
    "typescript": "5.9.3",
    "vitest": "4.0.18"
  }
}